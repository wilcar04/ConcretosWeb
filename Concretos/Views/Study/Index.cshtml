@model IEnumerable<Study>;
@{
    ViewData["Title"] = "Index";
}

    

<div class="container p-3">
    <div class="row pt-4">
        <div class="col-6">
            <h2 class="text-primary">Estudios Registrados</h2>
        </div>
        <div class="col-6 text-end">
            <a asp-controller="Study" asp-action="Create" class="btn btn-primary">
                <i class="bi bi-plus-circle"></i> &nbsp; Agregar nuevo estudio
            </a>
        </div>
    </div>
    <br /><br />


<table class="table table-bordered table-striped" style="width:100%">
        <thead>
            <tr>
                <th>ID</th>
                <th>Nombre del estudio</th>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
            @foreach(var obj in Model)
            {
                <tr>
                    <td><a href="#">@obj.Id</a></td>
                    <td>@obj.Name</td>
                    <td>@obj.CreatedDateTime</td>
                </tr>

            }

        </tbody>
</table>
</div>

